  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "common": "rm -rf node_modules/ && sleep 10 && npm install --legacy-peer-deps && npm run podSetup",
